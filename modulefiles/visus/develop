#%Module
#
proc ModulesHelp { } {
        puts stderr " "
        puts stderr "\t This module provides visus version HEAD"
        puts stderr " "
}

module-whatis   "VISUS (HEAD)"


set package_root /home/srinivm/CentOS7/software/visus/develop

prepend-path PATH "${package_root}/bin"
prepend-path LD_LIBRARY_PATH "${package_root}/lib"
prepend-path CMAKE_LIBRARY_PATH ${package_root}/lib
prepend-path CMAKE_INCLUDE_PATH ${package_root}/include

